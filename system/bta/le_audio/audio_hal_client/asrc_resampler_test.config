<configuration description="Config to run asrc_resampler_test unittests">
    <target_preparer class="com.android.tradefed.targetprep.PythonVirtualenvPreparer">
        <option name="dep-module" value="numpy" />
        <option name="dep-module" value="scipy" />
    </target_preparer>
    <test class="com.android.tradefed.testtype.mobly.MoblyBinaryHostTest" >
        <option name="mobly-par-file-name" value="asrc_resampler_test" />
        <option name="mobly-test-timeout" value="5m" />
    </test>
</configuration>
